<div class="row">
  <div class="row">
    <div class="col-md-3 col-sm-12 col-xs-12">
      <div class="panel panel-primary text-center no-boder bg-color-green">
        <div class="panel-body">
          <i class="fa  fa-eye fa-5x"></i>
          <h3>
            <%= Visit.all.count %>
          </h3>
        </div>
        <div class="panel-footer back-footer-green">
          Visits

        </div>
      </div>
    </div>
    <div class="col-md-3 col-sm-12 col-xs-12">
      <div class="panel panel-primary text-center no-boder bg-color-blue">
        <div class="panel-body">
          <i class="fa fa-bullhorn fa-5x"></i>
          <h3><%= News.all.count %> </h3>
        </div>
        <div class="panel-footer back-footer-blue">
          NEWS

        </div>
      </div>
    </div>
    <div class="col-md-3 col-sm-12 col-xs-12">
      <div class="panel panel-primary text-center no-boder bg-color-red">
        <div class="panel-body">
          <i class="fa fa fa-briefcase fa-5x"></i>
          <h3><%= Project.all.count %></h3>
        </div>
        <div class="panel-footer back-footer-red">
          Projects

        </div>
      </div>
    </div>
    <div class="col-md-3 col-sm-12 col-xs-12">
      <div class="panel panel-primary text-center no-boder bg-color-brown">
        <div class="panel-body">
          <i class="fa fa-refresh fa-5x"></i>
          <h3> <%= Feedback.all.count %> </h3>
        </div>
        <div class="panel-footer back-footer-brown">
          Feedbacks
        </div>
      </div>
    </div>

    <div class="col-md-3 col-sm-12 col-xs-12">
      <div class="panel panel-primary text-center no-boder bg-color-purple">
        <div class="panel-body">
          <i class="fa fa-gears fa-5x"></i>
          <h3> <%= Gadget.all.count %> </h3>
        </div>
        <div class="panel-footer back-footer-purple">
          Assets
        </div>
      </div>
    </div>

  </div>
